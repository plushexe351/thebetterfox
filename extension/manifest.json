{
  "manifest_version": 3,
  "name": "thebetterfox",
  "version": "0.1.0",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "host_permissions": [
    "https://suggestqueries.google.com/*"
  ],
  
  "permissions": [
    "storage",
    "tabs"
  ],
 "background": {
  "scripts":["background.js"]
},
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'; connect-src 'self' https://suggestqueries.google.com"
  }
}
